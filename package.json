{
  "name": "techwebsite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "github-build": "ng build --prod --base-href 'https://techwebsite.github.io'",
    "github-deploy": "npx angular-cli-ghpages --dir=dist/techwebsite",
    "github": "npm run github-build && npm run github-deploy",
    "firebase-build": "ng build --prod --aot",
    "firebase-deploy": "firebase deploy --only hosting",
    "firebase": "npm run firebase-build && npm run firebase-deploy",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "^12.2.15",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/fire": "^7.0.4",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.11",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@zoomus/websdk": "^2.1.1",
    "firebase": "^9.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular/cli": "^13.2.0",
    "@angular/compiler-cli": "^12.2.16",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.1",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
